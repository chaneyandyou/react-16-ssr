{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "react/jsx-filename-extension": [0],
    "import/no-extraneous-dependencies": [0],
    "import/no-unresolved": [0],
    "import/extensions": [0],
    "no-console": [0],
    "react/require-default-props": [0],
    "global-require": [0],
    "arrow-parens": [0],
    "func-names": [0],
    "no-underscore-dangle": [0],
    "arrow-body-style": [0],
    "no-param-reassign": [0],
    "consistent-return": [0]
  }
}
